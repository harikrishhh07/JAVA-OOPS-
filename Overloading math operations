class Mathoperations { 
    int add(int a, int b) { return a + b; }
    int sub(int a, int b) { return a - b; }
    int multi(int a, int b) { return a * b; }
    int div(int a, int b) { return a / b; }
}

public class Main {
    public static void main(String[] args) {
        Mathoperations o1 = new Mathoperations();
        System.out.println("The Addition of two numbers is: " + o1.add(10, 20));
        System.out.println("The Subtraction of two numbers is: " + o1.sub(50, 10));
        System.out.println("The Multiplication of two numbers is: " + o1.multi(5, 20));
        System.out.println("The Division of two numbers is: " + o1.div(40, 2));
    }
}
