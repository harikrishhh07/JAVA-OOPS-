class Mufasa {
    void king() {
        System.out.println("Brave");
        System.out.println("Leadership");
        System.out.println("Strong");
    }
}

class Simba extends Mufasa {
    Simba() {
        king();
    }
    void prince() {
        System.out.println("not yet ready to be a king");
    }
}

class Youngsimba extends Mufasa {
    Youngsimba() {
        king();
    }
    void futureprince() {
        System.out.println("Just now borned");
    }
}

public class Main {
    public static void main(String[] args) {
        System.out.println("Simba's Story:");
        Simba simba = new Simba();
        simba.prince();

        System.out.println("\nYoung Simba's Story:");
        Youngsimba young = new Youngsimba();
        young.futureprince();
    }
}
